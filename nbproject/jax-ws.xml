<?xml version="1.0" encoding="UTF-8"?>
<jax-ws xmlns="http://www.netbeans.org/ns/jax-ws/1">
  <services>
    <service name="ConcessionWS">
      <implementation-class>com.candlelabs.soast.webservice.ConcessionWS</implementation-class>
    </service>
    <service name="CourseWS">
      <implementation-class>com.candlelabs.soast.webservice.CourseWS</implementation-class>
    </service>
    <service name="NotificationWS">
      <implementation-class>com.candlelabs.soast.webservice.NotificationWS</implementation-class>
    </service>
    <service name="CirculationWS">
      <implementation-class>com.candlelabs.soast.webservice.CirculationWS</implementation-class>
    </service>
    <service name="PaymentOrderWS">
      <implementation-class>com.candlelabs.soast.webservice.PaymentOrderWS</implementation-class>
    </service>
    <service name="PersonWS">
      <implementation-class>com.candlelabs.soast.webservice.PersonWS</implementation-class>
    </service>
    <service name="ConstanciaWS">
      <implementation-class>com.candlelabs.soast.webservice.ConstanciaWS</implementation-class>
    </service>
    <service name="JefeDepartamento">
      <implementation-class>com.candlelabs.soast.webservice.JefeDepartamento</implementation-class>
    </service>
  </services>
  <clients>
    <client name="PersonWS">
      <wsdl-url>http://localhost:8080/PersonWS?WSDL</wsdl-url>
      <local-wsdl-file>localhost_8080/PersonWS.wsdl</local-wsdl-file>
      <package-name>com.candlelabs.soast.webservice</package-name>
      <catalog-file>catalog.xml</catalog-file>
      <wsimport-options>
        <wsimport-option>
          <wsimport-option-name>extension</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>verbose</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>fork</wsimport-option-name>
          <wsimport-option-value>false</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>wsdlLocation</wsimport-option-name>
          <wsimport-option-value>http://localhost:8080/PersonWS?WSDL</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>xnocompile</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>xendorsed</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
      </wsimport-options>
    </client>
  </clients>
</jax-ws>
